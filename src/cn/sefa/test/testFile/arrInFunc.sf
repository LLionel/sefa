def judge(arr,i){
    if arr[i]%2 == 0{
        print(arr[i]+" is even\n");
    }
    else{
        print(arr[i]+" is odd\n")
    }
}

def modifyEle(arr,index,val){
    print("before modifying arr[index]="+arr[index]+"\n")
    arr[index] = val
    print("modified successfully: arr[index]="+arr[index]+"\n")
}

def printArr(arr){
    len = length(arr)
    i = 0 
    while(i<len){
        judge(arr,i)
 //       print("arr["+i+"]="+arr[i]+"\n")
        val = arr[i]+100
        modifyEle(arr,i,val)    
        i = i + 1
    }
}

a = [1,2,[4,5,6],7,8]

