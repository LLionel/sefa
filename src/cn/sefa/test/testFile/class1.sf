class Position2D{
    x = 0
    y =0
    def move(nx , ny){
        this.x = nx
        y = ny
        closure(){
            z = this.x*this.y
        }
    }
}

class Position3D extends Position2D{
	z = 0;
	def move(nx , ny , nz){
		x = nx 
		y = ny
		z = nz
	}
	
}

p = Position3D.new
p.move(2,3,4)
print(p.x+p.y+p.z)
print(p.x*p.y*p.z+"...")